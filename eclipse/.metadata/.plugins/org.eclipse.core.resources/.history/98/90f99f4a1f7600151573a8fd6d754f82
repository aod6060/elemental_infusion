package com.derf.ei.item;

import net.minecraft.entity.player.EntityPlayer;
import net.minecraft.item.Item;
import net.minecraft.item.ItemStack;
import net.minecraft.world.World;

import com.derf.ei.EIType;
import com.derf.ei.Main;

public class EIItemDust extends Item {
	private EIType type;
	
	public EIItemDust(String name, EIType type) {
		super();
		
		this.setUnlocalizedName(name);
		
		this.setTextureName(Main.MODID + ":" + name);
		
		this.type  = type;
	}
	
	public EIType getType() {
		return type;
	}

	@Override
	public boolean onItemUse(ItemStack p_77648_1_, EntityPlayer p_77648_2_,
			World p_77648_3_, int p_77648_4_, int p_77648_5_, int p_77648_6_,
			int p_77648_7_, float p_77648_8_, float p_77648_9_,
			float p_77648_10_) {
		
		return true;
	}
	
	
}
