package com.derf.ei.multiblock;

import java.util.ArrayList;

import com.derf.ei.block.EIBlocks;

import net.minecraft.block.Block;
import net.minecraft.init.Blocks;
import net.minecraft.world.World;

public class EIMultiBlockFireTotem extends EIMultiBlockTotem {
	
	public EIMultiBlockFireTotem() {
		this.addBlockRecipe(Blocks.iron_block, EIBlocks.fireIronBlock, 3.0f);
		this.addBlockRecipe(Blocks.diamond_block, EIBlocks.fireDiamondBlock, 6.0f);
	}
}
